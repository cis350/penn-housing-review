{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb", 
        "prettier"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module", 
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "plugins": [
        "prettier",
        "react"
    ],
    "rules": {
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }], 
        "jsx-a11y/anchor-is-valid": 0, 
        "react/prop-types": "off",
        "no-param-reassign": 0, 
        "no-underscore-dangle": "off"
    }
}
