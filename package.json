    "axios-mock-adapter": "^1.21.2",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "jest": "^29.5.0",
    "mongodb": "^5.2.0",
    "path": "^0.12.7",
    "prettier-eslint": "^15.0.1",
    "prettier-eslint-cli": "^7.1.0",
    "prop-types": "^15.8.1",
    "rc-slider": "^10.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.9.0",
    "react-scripts": "5.0.1",
    "styled-components": "^5.3.9",
    "supertest": "^6.3.3",
